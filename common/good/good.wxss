/* common/good/good.wxss */

.box{
  background-color: #fff;
  border: 1px solid #d2d2d4;
  margin:0 10rpx 40rpx 10rpx;
  padding: 20rpx;
  font-size: 13px;
}
.box-img{
  width: 220rpx;
  height: 220rpx;
  margin-right: 30rpx;
}
.t-red{
  color: #fe0000;
}
.t-blue{
  color:#0297fe;
}
.t-left{
  text-align: left;
}
.t-gray{
  color: #808080;
}
.df{
  display: flex;
  align-items: center;
}
.df-sb{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.t-bold{
  font-weight: 700;
}
.h50{
  height: 50rpx;
  line-height: 50rpx;
  display: block;
}
.t-nowrap{
  white-space:nowrap;
}
.t-big{
  font-size: 16px;
}