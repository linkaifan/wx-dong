/* pages/evaluation/evaluation.wxss */
#evaluation{
  font-size: 13px;
}
.e-img{
  width: 130rpx;
  height: 130rpx;
  margin-right: 30rpx;
}
.star.active{
  color: #0297fe;
  border-color: #0297fe;
}
.star{
  font-size: 11px;
  width: 85rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border: 1px solid #d2d2d4;
  margin-right: 15rpx;
}
.e-btn{
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #fff;
  background-color: #0297fe;
  text-align: center;
  border-radius: 5px;
  margin-top: 40rpx;
}